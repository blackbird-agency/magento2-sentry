{
    "name": "blackbird/magento2-sentry",
    "description": "Magento 2 Logger for Sentry",
    "type": "magento2-module",
    "version": "103.7.2-p4",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "sentry/sdk": "^4.0",
        "monolog/monolog": ">=2.7.0",
        "magento/framework": "*",
        "nyholm/psr7": "^1.2",
        "magento/module-config": "^101.2",
        "blackbird/module-nonce-provider": "^1.0.0"
    },
    "authors": [
        {
            "name": "Rakhal Imming",
            "email": "rakhal@justbetter.nl",
            "homepage": "https://justbetter.nl",
            "role": "Developer"
        },
        {
            "name": "Joseph Maxwell",
            "email": "joseph@swiftotter.com",
            "homepage": "https://swiftotter.com",
            "role": "Developer"
        },
        {
            "name": "Blackbird Agency",
            "email": "contact@bird.eu",
            "homepage": "https://black.bird.eu"
        }
    ],
    "autoload": {
        "psr-4": {
            "JustBetter\\Sentry\\": ""
        },
        "files": [
            "registration.php"
        ]
    },
    "replace": {
        "justbetter/magento2-sentry": "*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "magento/composer-dependency-version-audit-plugin": true
        }
    },
    "require-dev": {
        "bitexpert/phpstan-magento": "^0.11.0",
        "phpstan/phpstan": "^1.10"
    }
}
